{
  "name": "yacdn",
  "version": "0.1.0",
  "description": "Yet Another CDN.",
  "main": "server.js",
  "scripts": {
    "test": "npm run testsuite && npm run linter",
    "testsuite": "DEBUG=yacdn:* jest --forceExit",
    "coverage": "jest --forceExit --coverage",
    "linter": "xo",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovsoinc/yacdn.git"
  },
  "author": "",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ovsoinc/yacdn/issues"
  },
  "homepage": "https://github.com/ovsoinc/yacdn#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "debug": "^4.1.1",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "human-readable-numbers": "^0.9.5",
    "ioredis": "^4.14.0",
    "koa": "^2.8.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "mem": "^5.1.1",
    "mime-types": "^2.1.24",
    "nodemon": "^1.19.1",
    "pretty-bytes": "git+https://git@github.com/ovsoinc/pretty-bytes#bf25220869dd18b71f7a69709040b7940e7609bb",
    "request": "^2.88.0",
    "winston": "^3.2.1",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "del": "^5.1.0",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^24.9.0",
    "mocha": "^7.0.1",
    "mocker": "^0.1.2",
    "xo": "^0.24.0"
  },
  "xo": {
    "globals": [
      "window",
      "describe",
      "it",
      "before"
    ],
    "rules": {
      "no-await-in-loop": "off",
      "curly": "off",
      "capitalized-comments": "off",
      "no-new": "off",
      "prefer-rest-params": "off",
      "unicorn/filename-case": [
        "off",
        {
          "case": "camelCase"
        }
      ],
      "camelcase": "off"
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
